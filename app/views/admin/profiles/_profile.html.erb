<tr id="profile_<%= profile.id %>">
  <td><%= profile.name %></td>
  <td><%= profile.contact %></td>
  <td><%= profile.email %></td>
  <td><%= profile.qq %></td>
  <td><%= link_to edit_admin_profile_path(profile),remote: true,
  class: 'ui button basic icon small green' do  %>
      <i class="edit icon"></i>修改
      <% end %>
   </td>
   <td><%= link_to admin_profile_path(profile),
   method: :delete , data: {confirm: 'Are you Sure?'}, 
   class: 'ui button basic icon small red' do %>
      <i class="trash icon"></i>删除
      <% end %>
   </td>
</tr>